@using PurpleBuzz_Backend.Models
@model ContactIntro
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">

                        <div class="form-group ">
                            <div class="d-flex justify-content-center"><img src="~/assets/img/@Model.PhotoName" /></div>
                        </div>
                        <div class="form-group">
                            <label asp-for="Title">Title</label>
                            <input class="form-control" asp-for="Title" disabled>
                        </div>
                        <div class="form-group">
                            <label asp-for="PrimaryText">Primary Text</label>
                            <input class="form-control" asp-for="PrimaryText" disabled>
                        </div>
                        <div class="form-group">
                            <label asp-for="Description">Description</label>
                            <input class="form-control" asp-for="Description" disabled>
                        </div>
                      
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>